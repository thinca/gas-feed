{
  "name": "gas-feed",
  "description": "Feed reader library for Google Apps Script",
  "version": "1.0.8",
  "author": "thinca <thinca+npm@gmail.com>",
  "bugs": {
    "url": "https://github.com/thinca/gas-feed/issues"
  },
  "dependencies": {
    "@types/google-apps-script": "1.0.14"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "files": [
    "built/"
  ],
  "homepage": "https://github.com/thinca/gas-feed",
  "keywords": [
    "Google Apps Script",
    "feed",
    "rss"
  ],
  "license": "Zlib",
  "main": "./built/index.js",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "all": true,
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinca/gas-feed.git"
  },
  "scripts": {
    "clean": "rm -fr built",
    "compile": "tsc",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.test.ts'",
    "test": "nyc mocha ./test/**/*.test.ts"
  },
  "types": "./built/index.d.ts"
}
