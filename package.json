{
  "name": "gas-feed",
  "description": "Feed reader library for Google Apps Script",
  "version": "1.0.6",
  "author": "thinca <thinca+npm@gmail.com>",
  "bugs": {
    "url": "https://github.com/thinca/gas-feed/issues"
  },
  "dependencies": {
    "@types/google-apps-script": "1.0.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  },
  "files": [
    "built/"
  ],
  "homepage": "https://github.com/thinca/gas-feed",
  "keywords": [
    "Google Apps Script",
    "feed",
    "rss"
  ],
  "license": "Zlib",
  "main": "./built/index.js",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "all": true,
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinca/gas-feed.git"
  },
  "scripts": {
    "clean": "rm -fr built",
    "compile": "tsc",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "test": "nyc mocha ./test/**/*.test.ts"
  },
  "types": "./built/index.d.ts"
}
